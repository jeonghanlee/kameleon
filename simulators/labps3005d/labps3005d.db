# description: EPICS database records (i.e. PVs) for the LABPS3005D simulator.



record(bo, "$(PREFIX)_POWER_CMD")
{
   field(DTYP, "stream")
   field(OUT, "@$(PROTOCOL) set_power $(PORT)")
   field(ZNAM, "0")
   field(ONAM, "1")
}


record(bi, "$(PREFIX)_POWER_STS")
{
   field(DTYP, "stream")
   field(INP, "@$(PROTOCOL) get_power $(PORT)")
   field(PINI, "YES")
   field(ZNAM, "0")
   field(ONAM, "1")
}


record(ai, "$(PREFIX)_FIXED_STS")
{
   field(DTYP, "stream")
   field(INP, "@$(PROTOCOL) get_fixed $(PORT)")
   field(PINI, "YES")
   field(SCAN, "1 second")
   field(PROC, "1")
}


record(ai, "$(PREFIX)_ENUM_STS")
{
   field(DTYP, "stream")
   field(INP, "@$(PROTOCOL) get_enum $(PORT)")
   field(PINI, "YES")
   field(SCAN, "1 second")
}
